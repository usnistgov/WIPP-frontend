{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "wipp-frontend-lib": {
      "root": "projects/wipp-frontend-lib",
      "sourceRoot": "projects/wipp-frontend-lib/src",
      "projectType": "library",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/wipp-frontend-lib/tsconfig.lib.json",
            "project": "projects/wipp-frontend-lib/ng-package.json"
          }
        ,          "configurations": {
            "production": {
              "tsConfig": "projects/wipp-frontend-lib/tsconfig.lib.prod.json"
            }
          }
},
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/wipp-frontend-lib/src/test.ts",
            "tsConfig": "projects/wipp-frontend-lib/tsconfig.spec.json",
            "karmaConfig": "projects/wipp-frontend-lib/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/wipp-frontend-lib/tsconfig.lib.json",
              "projects/wipp-frontend-lib/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "wipp-frontend-app": {
      "root": "projects/wipp-frontend-app/",
      "sourceRoot": "projects/wipp-frontend-app/src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "aot": true,
            "outputPath": "dist/wipp-frontend-app",
            "index": "projects/wipp-frontend-app/src/index.html",
            "main": "projects/wipp-frontend-app/src/main.ts",
            "polyfills": "projects/wipp-frontend-app/src/polyfills.ts",
            "tsConfig": "projects/wipp-frontend-app/tsconfig.app.json",
            "assets": [
              "projects/wipp-frontend-app/src/favicon.ico",
              "projects/wipp-frontend-app/src/assets",
              { "glob": "**/*", "input": "node_modules/@wipp/wdzt/build/deps/images", "output": "./assets/wdzt/images" }
            ],
            "styles": [
              {
                "input": "node_modules/@angular/material/prebuilt-themes/indigo-pink.css"
              },
              "projects/wipp-frontend-app/src/styles.css",
              "node_modules/@wipp/wdzt/build/deps/wdzt-deps.css",
              "node_modules/prismjs/themes/prism.css"            ],
            "scripts": [
              "node_modules/@wipp/wdzt/build/deps/wdzt-deps.js",
              "node_modules/keycloak-js/dist/keycloak.js",
              "node_modules/notebookjs/notebook.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/wipp-frontend-app/src/environments/environment.ts",
                  "with": "projects/wipp-frontend-app/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "wipp-frontend-app:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "wipp-frontend-app:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "wipp-frontend-app:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/wipp-frontend-app/src/test.ts",
            "polyfills": "projects/wipp-frontend-app/src/polyfills.ts",
            "tsConfig": "projects/wipp-frontend-app/tsconfig.spec.json",
            "karmaConfig": "projects/wipp-frontend-app/karma.conf.js",
            "styles": [
              "projects/wipp-frontend-app/src/styles.css"
            ],
            "scripts": [],
            "assets": [
              "projects/wipp-frontend-app/src/favicon.ico",
              "projects/wipp-frontend-app/src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/wipp-frontend-app/tsconfig.app.json",
              "projects/wipp-frontend-app/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "wipp-frontend-app-e2e": {
      "root": "projects/wipp-frontend-app-e2e/",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/wipp-frontend-app-e2e/protractor.conf.js",
            "devServerTarget": "wipp-frontend-app:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "wipp-frontend-app:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "projects/wipp-frontend-app-e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "wipp-frontend-app",
  "cli": {
    "analytics": false
  }
}